<p id="notice"><%= notice %></p>

<h1>Listing Polititians</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @polititians.each do |polititian| %>
      <tr>
        <td><%= link_to 'Show', polititian %></td>
        <td><%= link_to 'Edit', edit_polititian_path(polititian) %></td>
        <td><%= link_to 'Destroy', polititian, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Polititian', new_polititian_path %>
